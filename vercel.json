{"version": 2, "builds": [{"src": "index.html", "use": "@vercel/static"}, {"src": "backend/server.js", "use": "@vercel/node"}, {"src": "enhanced-features.js", "use": "@vercel/static"}, {"src": "animations.css", "use": "@vercel/static"}, {"src": "frontend/api.js", "use": "@vercel/static"}], "routes": [{"src": "/api/(.*)", "dest": "/backend/server.js/$1"}, {"src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$", "dest": "/$1"}, {"src": "/(.*)", "dest": "/index.html"}], "env": {"NODE_ENV": "production", "MONGODB_URI": "@mongodb_uri", "JWT_SECRET": "@jwt_secret", "EMAIL_USER": "@email_user", "EMAIL_PASS": "@email_pass", "BUSINESS_EMAIL": "@business_email", "BUSINESS_PHONE": "@business_phone", "WHATSAPP_PHONE": "@whatsapp_phone", "CLIENT_URL": "@client_url"}, "headers": [{"source": "/(.*)", "headers": [{"key": "X-Frame-Options", "value": "DENY"}, {"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "Referrer-Policy", "value": "strict-origin-when-cross-origin"}, {"key": "Access-Control-Allow-Origin", "value": "*"}, {"key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS"}, {"key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization"}]}]}